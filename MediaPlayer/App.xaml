<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="MediaPlayer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MediaPlayer">
    <Application.Resources>
        <ResourceDictionary>
            <local:DurationToStringConverter x:Key="DurationToStringConverter"/>
            <local:EmptyStringToDefaultConverter x:Key="EmptyStringToDefaultConverter"/>
            <local:IsSelectedColorConverter x:Key="IsSelectedColorConverter"/>
            <local:DateToStringConverter x:Key="DateToStringConverter"/>
            <local:PlayerService x:Key="PlayerService"/>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
